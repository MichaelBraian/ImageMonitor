rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    match /patients/{patientId}/{allPaths=**} {
      allow read: if request.auth != null && 
        request.auth.token.email != null;
      allow write: if request.auth != null && 
        request.auth.token.email != null;
    }
  }
}
